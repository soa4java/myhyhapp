<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <include
        android:id="@+id/i_head"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/header_height"
        layout="@layout/common_head" />

    <ScrollView
        android:id="@+id/becomeshangjia_scrollView"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:id="@+id/becomeshangjia_middle"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@color/white"
            android:orientation="vertical" >

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="21dp"
                android:background="@color/colorf8f8f8"
                android:gravity="center_vertical"
                android:paddingLeft="18dp"
                android:text="@string/jibenxinxi"
                android:textColor="@color/color949494"
                android:textSize="@dimen/size10" />

            <RelativeLayout
                android:id="@+id/rl_shop_name"
                android:layout_width="fill_parent"
                android:layout_height="48dp"
                android:background="@color/white"
                android:paddingLeft="18dp"
                android:paddingRight="18dp" >

                <TextView
                    android:id="@+id/tv_shop_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:text="商家名称"
                    android:textColor="@color/color323232"
                    android:textSize="@dimen/size14" />

                <EditText
                    android:id="@+id/ed_shop_name"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/view_select"
                    android:paddingLeft="90dp"
                    android:textColor="@color/selector_textcolor"
                    android:textSize="@dimen/size14" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/becomeshangjia_qiyemingcheng"
                android:layout_width="fill_parent"
                android:layout_height="48dp"
                android:clickable="true"
                android:paddingLeft="18dp"
                android:paddingRight="18dp" >

                <TextView
                    android:id="@+id/tv_becomeshangjia_qiyemingcheng"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:text="企业类型"
                    android:textColor="@color/color323232"
                    android:textSize="@dimen/size14" />

                <TextView
                    android:id="@+id/tv_becomeshangjia_qiyeleixingchoose"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="90dp"
                    android:text="@string/qingxuanze"
                    android:textColor="@color/color949494"
                    android:textSize="@dimen/size14" />

                <ImageView
                    android:id="@+id/iv_becomeshangjia_qiyeleixingchoose"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="6dp"
                    android:src="@drawable/common_arrow" />
            </RelativeLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="18dp"
                android:layout_marginRight="18dp"
                android:background="@color/colorececec" />

            <RelativeLayout
                android:id="@+id/rl_linkman"
                android:layout_width="fill_parent"
                android:layout_height="48dp"
                android:background="@color/white"
                android:paddingLeft="18dp"
                android:paddingRight="18dp" >

                <TextView
                    android:id="@+id/tv_linkman"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:text="联系人"
                    android:textColor="@color/color323232"
                    android:textSize="@dimen/size14" />

                <EditText
                    android:id="@+id/ed_linkman"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/view_select"
                    android:paddingLeft="90dp"
                    android:textColor="@color/selector_textcolor"
                    android:textSize="@dimen/size14" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_numbers"
                android:layout_width="fill_parent"
                android:layout_height="48dp"
                android:background="@color/white"
                android:paddingLeft="18dp"
                android:paddingRight="18dp" >

                <TextView
                    android:id="@+id/tv_numbers"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="6dp"
                    android:text="身份证号"
                    android:textColor="@color/color323232"
                    android:textSize="@dimen/size14" />

                <EditText
                    android:id="@+id/ed_numbers"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/view_select"
                    android:paddingLeft="90dp"
                    android:textColor="@color/selector_textcolor"
                    android:textSize="@dimen/size14" />
            </RelativeLayout>
           
            <RelativeLayout
                android:id="@+id/rl_company_phone"
                android:layout_width="fill_parent"
                android:layout_height="48dp"
                android:background="@color/white"
                android:paddingLeft="18dp"
                android:paddingRight="18dp" >

                <TextView
                    android:id="@+id/tv_company_phone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:text="企业电话"
                    android:textColor="@color/color323232"
                    android:textSize="@dimen/size14" />

                <EditText
                    android:id="@+id/ed_company_phone"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/view_select"
                    android:paddingLeft="90dp"
                    android:textColor="@color/selector_textcolor"
                    android:textSize="@dimen/size14" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_company_zhanghao"
                android:layout_width="fill_parent"
                android:layout_height="48dp"
                android:background="@color/white"
                android:paddingLeft="18dp"
                android:paddingRight="18dp" >

                <TextView
                    android:id="@+id/tv_company_zhanghao"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:text="收款账号"
                    android:textColor="@color/color323232"
                    android:textSize="@dimen/size14" />

                <EditText
                    android:id="@+id/ed_company_zhanghao"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/view_select"
                    android:paddingLeft="90dp"
                    android:textColor="@color/selector_textcolor"
                    android:textSize="@dimen/size14" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_company_kaihuname"
                android:layout_width="fill_parent"
                android:layout_height="48dp"
                android:background="@color/white"
                android:paddingLeft="18dp"
                android:paddingRight="18dp" >

                <TextView
                    android:id="@+id/tv_company_kaihuname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:text="开户姓名"
                    android:textColor="@color/color323232"
                    android:textSize="@dimen/size14" />

                <EditText
                    android:id="@+id/ed_company_kaihuname"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/view_select"
                    android:paddingLeft="90dp"
                    android:textColor="@color/selector_textcolor"
                    android:textSize="@dimen/size14" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_company_kaihuhang"
                android:layout_width="fill_parent"
                android:layout_height="48dp"
                android:background="@color/white"
                android:paddingLeft="18dp"
                android:paddingRight="18dp" >

                <TextView
                    android:id="@+id/tv_company_kaihuhang"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:text="开户行"
                    android:textColor="@color/color323232"
                    android:textSize="@dimen/size14" />

                <EditText
                    android:id="@+id/ed_company_kaihuhang"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/view_select"
                    android:paddingLeft="90dp"
                    android:textColor="@color/selector_textcolor"
                    android:textSize="@dimen/size14" />
            </RelativeLayout>
			<RelativeLayout
                android:id="@+id/rl_time"
                android:layout_width="fill_parent"
                android:layout_height="48dp"
                android:background="@color/white"
                android:paddingLeft="18dp"
                android:paddingRight="18dp" >

                <TextView
                    android:id="@+id/tv_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:text="营业时间"
                    android:textColor="@color/color323232"
                    android:textSize="@dimen/size14" />

                <EditText
                    android:id="@+id/ed_time"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/view_select"
                    android:paddingLeft="90dp"
                    android:textColor="@color/selector_textcolor"
                    android:textSize="@dimen/size14" />
                <TextView
                    android:id="@+id/ed_time_start"
                    android:layout_width="60dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center_vertical"
                    android:background="@android:color/transparent"
                    android:layout_marginLeft="90dp"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="开始"
                    android:textColor="@color/selector_textcolor"
                    android:textSize="@dimen/size14" />
                <TextView android:text="-"
                    android:textColor="@color/color323232"
                    android:textSize="12sp"
                    android:padding="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:gravity="center_vertical|center_horizontal"
                    android:id="@+id/tv_horizontal_line"
                    android:layout_toRightOf="@+id/ed_time_start"/>
                <TextView
                    android:id="@+id/ed_time_end"
                    android:layout_toRightOf="@+id/tv_horizontal_line"
                    android:layout_width="60dp"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center_vertical"
                    android:background="@android:color/transparent"
                    android:text="结束"
                    android:gravity="center_vertical|center_horizontal"
                    android:textColor="@color/selector_textcolor"
                    android:textSize="@dimen/size14" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_issend"
                android:layout_width="fill_parent"
                android:layout_height="48dp"
                android:background="@color/white"
                android:paddingLeft="18dp"
                android:clickable="true"
                android:paddingRight="18dp" >

                <TextView
                    android:id="@+id/tv_issend"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:text="上门服务"
                    android:textColor="@color/color323232"
                    android:textSize="@dimen/size14" />

                <TextView
                    android:id="@+id/tv_issend_val"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="90dp"
                    android:text="@string/qingxuanze"
                    android:textColor="@color/color949494"
                    android:textSize="@dimen/size14" />

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="6dp"
                    android:src="@drawable/common_arrow" />
            </RelativeLayout>
            <TextView
                android:layout_width="fill_parent"
                android:layout_height="21dp"
                android:layout_marginTop="6dp"
                android:background="@color/colorf8f8f8"
                android:gravity="center_vertical"
                android:paddingLeft="18dp"
                android:text="@string/qiyerenzheng"
                android:textColor="@color/color949494"
                android:textSize="@dimen/size10" />

            <RelativeLayout
                android:id="@+id/becomeshangjia_yinyezhizhao"
                android:layout_width="fill_parent"
                android:layout_height="61dp"
                android:background="@color/white"
                android:paddingLeft="18dp"
                android:paddingRight="18dp" >

                <TextView
                    android:id="@+id/tv_becomeshangjia_yinyezhizhao"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:text="@string/yingyezhizhao"
                    android:textColor="@color/color323232"
                    android:textSize="@dimen/size14" />

                <com.gezitech.widget.RemoteImageView
                    android:id="@+id/iv_yinyezhizhao"
                    android:layout_width="42dp"
                    android:layout_height="42dp"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="12dp"
                    android:layout_toLeftOf="@+id/btn_becomeshangjia_yinyezhizhao"
                     />

                <Button
                    android:id="@+id/btn_becomeshangjia_yinyezhizhao"
                    android:layout_width="42dp"
                    android:layout_height="42dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/button_cwsj_add" />
            </RelativeLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="18dp"
                android:layout_marginRight="18dp"
                android:background="@color/colorececec" />

            <RelativeLayout
                android:id="@+id/becomeshangjia_jingyingxukezheng"
                android:layout_width="fill_parent"
                android:layout_height="61dp"
                android:background="@color/white"
                android:paddingLeft="18dp"
                android:paddingRight="18dp" >

                <TextView
                    android:id="@+id/tv_becomeshangjia_jingyingxukezheng"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:text="@string/jingyingxukezheng"
                    android:textColor="@color/color323232"
                    android:textSize="@dimen/size14" />

                <com.gezitech.widget.RemoteImageView
                    android:id="@+id/iv_jingyingxukezheng"
                    android:layout_width="42dp"
                    android:layout_height="42dp"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="12dp"
                    android:layout_toLeftOf="@+id/btn_becomeshangjia_jingyingxukezheng"
                     />

                <Button
                    android:id="@+id/btn_becomeshangjia_jingyingxukezheng"
                    android:layout_width="42dp"
                    android:layout_height="42dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/button_cwsj_add" />
            </RelativeLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="18dp"
                android:layout_marginRight="18dp"
                android:background="@color/colorececec" />

            <RelativeLayout
                android:id="@+id/becomeshangjia_changdizhaopian"
                android:layout_width="fill_parent"
                android:layout_height="61dp"
                android:background="@color/white"
                android:paddingLeft="18dp"
                android:paddingRight="18dp" >

                <TextView
                    android:id="@+id/tv_becomeshangjia_changdizhaopian"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:text="@string/changdizhaopian"
                    android:textColor="@color/color323232"
                    android:textSize="@dimen/size14" />

                <LinearLayout android:id="@+id/ll_changdizhaopian_box"
                    android:layout_width="fill_parent"
                    android:layout_height="50dp"
                    android:orientation="horizontal"
                    android:layout_centerVertical="true"
                    android:gravity="right|center_vertical"
                    android:layout_toLeftOf="@+id/btn_becomeshangjia_changdizhaopian"
                    android:layout_marginRight="12dp">
                </LinearLayout>
                <Button
                    android:id="@+id/btn_becomeshangjia_changdizhaopian"
                    android:layout_width="42dp"
                    android:layout_height="42dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/button_cwsj_add" />
            </RelativeLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="18dp"
                android:layout_marginRight="18dp"
                android:background="@color/colorececec" />

            <RelativeLayout
                android:id="@+id/becomeshangjia_lianxirenzhaopian"
                android:layout_width="fill_parent"
                android:layout_height="61dp"
                android:background="@color/white"
                android:paddingLeft="18dp"
                android:paddingRight="18dp" >

                <TextView
                    android:id="@+id/tv_becomeshangjia_lianxirenzhaopian"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="6dp"
                    android:text="@string/lianxirenzhaopian"
                    android:textColor="@color/color323232"
                    android:textSize="@dimen/size14" />

                <com.gezitech.widget.RemoteImageView
                    android:id="@+id/iv_lianxirenzhaopian"
                    android:layout_width="42dp"
                    android:layout_height="42dp"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="12dp"
                    android:layout_toLeftOf="@+id/btn_becomeshangjia_lianxirenzhaopian"/>

                <Button
                    android:id="@+id/btn_becomeshangjia_lianxirenzhaopian"
                    android:layout_width="42dp"
                    android:layout_height="42dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/button_cwsj_add" />
            </RelativeLayout>
            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="18dp"
                android:layout_marginRight="18dp"
                android:background="@color/colorececec" />

            <LinearLayout
                android:id="@+id/becomeshangjia_bottom"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="@color/colorf8f8f8"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24dp"
                    android:layout_marginRight="24dp"
                    android:layout_marginTop="10dp"
                    android:gravity="center_vertical|left"
                    android:orientation="horizontal" >

                    <CheckBox
                        android:id="@+id/bt_remember_agree"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:background="@android:color/transparent"
                        android:button="@null"
                        android:checked="true"
                        android:drawableLeft="@drawable/checkbox_background2"
                        android:drawablePadding="6dp"
                        android:text="同意"
                        android:textColor="@color/selector_color"
                        android:textSize="@dimen/size14" />

                    <TextView
                        android:id="@+id/tv_shop_service"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="商家服务协议"
                        android:textColor="@color/color3CA5F3"
                        android:textSize="@dimen/size14" />
                </LinearLayout>

                <TextView
                    android:id="@+id/tv_yuanyin"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="24dp"
                    android:layout_marginRight="24dp"
                    android:layout_marginTop="10dp"
                    android:visibility="gone"
                    android:text=""
                    android:textColor="#e62d2d"
                    android:textSize="@dimen/size14" />

                <Button
                    android:id="@+id/Login_login"
                    android:layout_width="fill_parent"
                    android:layout_height="42dp"
                    android:layout_marginBottom="24dp"
                    android:layout_marginLeft="24dp"
                    android:layout_marginRight="24dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/button_common_btn_blue"
                    android:text="@string/tijiaorenzheng"
                    android:textColor="@color/white"
                    android:textSize="@dimen/size18" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>